/**index.wxss**/
.head_top{
  box-sizing: border-box;
  overflow: hidden;
  width: 100%;
  height: 350rpx;
  padding: 10rpx 25rpx;
}
.head_top .head_left image{
  width: 55rpx;
  height: 55rpx;
}
.head_center{
  width: 550rpx;
  height: 55rpx;
  margin-left: 20rpx;
  position: relative;
}
.head_center input{
  border: 4rpx solid #fff;
  border-radius: 30rpx;
  color: #fff;
  height: 55rpx;
  font-size: 28rpx;
  text-align: center;
}

.head_center .search{
  position: absolute;
  right: 45rpx;
  top: 10rpx;
  width: 45rpx;
  height: 45rpx;
}
.head_right{
  width: 53rpx;
  height: 43rpx;
  margin-top: 10rpx;
}
.fl{
  margin: 30rpx 0;
}
.fl_list{
  width: 33.333%;
  text-align: center;
}
.fl_list image{
  width: 139rpx;
  height: 138rpx;
}
.fl_list .fl_title{
  font-size: 26rpx;
  color: #bc8f30;
  font-weight: bold;
}
.fj{
  margin-top: 60rpx;
  box-sizing: border-box;
  padding: 0 30rpx;
  margin-bottom: 20rpx;
}
.fj_left{
  font-size: 36rpx;
  color: #bc8f30;
}
.fj_right{
  width: 130rpx;
  height: 40rpx;
  line-height: 40rpx;
  border-radius: 30rpx;
  color: #fff;
  font-size: 24rpx;
  background: #c19f79;
  text-align: center;
  margin-top: 10rpx;
}
.fj_content{
  width: 100%;
  height: 327rpx;
  position: relative;
  box-sizing: border-box;
}
.fj_content .like{
  position: absolute;
  width: 32rpx;
  height: 30rpx;
  top: 30rpx;
  right: 30rpx;
}
.money{
  position: absolute;
  bottom: 10rpx;
  left: 0;
  padding: 0 30rpx;
  color: #fff;
  font-size: 30rpx;
  width: 100%;
  box-sizing: border-box;
}
.money_left{
  width: 50%;
}
.money_right{
  text-align: right;
}
.money_right image{
  width: 20rpx;
  height: 20rpx;
  margin-right: 5rpx;
}